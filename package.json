{
  "name": "gales-sales-server",
  "version": "2.0.0",
  "description": "Gales API Server",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "babel src -d build",
    "lint": "eslint './src/**/*.js'",
    "lint:fix": "eslint './src/**/*.js' --fix",
    "serve": "NODE_ENV=development node build/index.js",
    "start": "NODE_ENV=test nodemon src/local.index.js --exec babel-node",
    "start:dev": "NODE_ENV=development nodemon src/local.index.js --exec babel-node",
    "test": "jest",
    "test:all": "jest src",
    "test:w": "jest --watch",
    "test:clear": "jest --clearCache",
    "coverage": "jest --collectCoverageFrom=src/**.js --coverage src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pulpfree/gales-sales-server"
  },
  "author": "Ron Dyck <rond@webbtech.net>",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "7.6.0",
    "@hapi/boom": "7.4.3",
    "@hapi/hapi": "18.4.0",
    "@thundra/core": "2.7.0",
    "aws-sdk": "2.496.0",
    "co": "4.6.0",
    "co-foreach": "1.1.1",
    "flat": "4.1.0",
    "glob": "7.1.4",
    "js-yaml": "3.13.1",
    "moment": "2.24.0",
    "mongo-sanitize": "1.0.1",
    "mongoose": "5.7.13",
    "node-jose": "1.1.3",
    "ramda": "0.26.1"
  },
  "devDependencies": {
    "@babel/cli": "7.6.0",
    "@babel/core": "7.6.0",
    "@babel/node": "7.6.1",
    "@babel/plugin-transform-runtime": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "eslint": "6.4.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "jest": "24.9.0",
    "nodemon": "1.19.2"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
