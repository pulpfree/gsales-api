{
  "name": "gales-sales-server",
  "version": "2.0.0",
  "description": "Gales API Server",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "babel src -d dist",
    "lint": "eslint './src/**/*.js'",
    "lint:fix": "eslint './src/**/*.js' --fix",
    "serve": "NODE_ENV=development node dist/index.js",
    "start": "NODE_ENV=test nodemon src/index.js --exec babel-node",
    "start:dev": "NODE_ENV=development nodemon src/index.js --exec babel-node",
    "test": "jest",
    "test:all": "jest src",
    "test:w": "jest --watch",
    "test:clear": "jest --clearCache",
    "coverage": "jest --collectCoverageFrom=src/**.js --coverage src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pulpfree/gales-sales-server"
  },
  "author": "Ron Dyck <rond@webbtech.net>",
  "license": "ISC",
  "dependencies": {
    "@hapi/hapi": "18.3.1",
    "aws-sdk": "2.496.0",
    "boom": "7.3.0",
    "co": "4.6.0",
    "co-foreach": "1.1.1",
    "flat": "4.1.0",
    "glob": "7.1.4",
    "hoek": "6.1.3",
    "joi": "14.3.1",
    "js-yaml": "3.13.1",
    "moment": "2.24.0",
    "mongo-sanitize": "1.0.1",
    "mongoose": "5.6.5",
    "ramda": "0.26.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-loader": "8.0.6",
    "babel-preset-env": "1.7.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-plugin-import": "2.18.2",
    "jest": "24.8.0",
    "mongoose-mock": "0.4.0",
    "nodemon": "1.19.1"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
